func inline void putchar (char) {
    var tmp = [vga_mem + cursorPositionX + (cursorPositionY * CONSOLE_WIDTH)];
    _reg_assign(0, char);
    _reg_assign(1, tmp);
    _asm {
        MEMW A B
    }
}

func void alphabet () {
    var curChar = ASCII_A;

    while [curChar <= ASCII_Z] {
        putchar(curChar);
        curChar += 1;
        cursorPositionX += 1;
    }
}